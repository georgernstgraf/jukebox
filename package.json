{
    "name": "jukebox",
    "version": "1.0.0",
    "description": "wu mp3 collection",
    "main": "app.ts",
    "type": "module",
    "scripts": {
        "start": "npx tsc && npm run css && node --env-file=.env dist/app.js",
        "devstart": "node --env-file=.env dist/app.js",
        "tsc": "npx tsc",
        "css": "npx purgecss --content 'partials/**/*.hbs' --content 'static/js/*.js' -o static/css --css raw/*.css -v",
        "devcss": "cp raw/* static/css",
        "dev": "npx nodemon"
    },
    "prisma": {
        "seed": "npm run seed"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/georgernstgraf/jukebox.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/georgernstgraf/jukebox/issues"
    },
    "homepage": "https://github.com/georgernstgraf/jukebox#readme",
    "dependencies": {
        "@hono/node-server": "^1.15.0",
        "@prisma/client": "^6.11.1",
        "@types/node": "^24.0.13",
        "authenticate-pam": "^1.0.5",
        "bcrypt": "^6.0.0",
        "file-type": "^21.0.0",
        "handlebars": "^4.7.8",
        "hono": "^4.8.4",
        "memjs": "^1.3.2",
        "music-metadata": "^11.6.0",
        "prisma": "^6.11.1",
        "purgecss": "^7.0.2",
        "typescript": "^5.8.3",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/memjs": "^1.3.3",
        "nodemon": "^3.1.10"
    }
}